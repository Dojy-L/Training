<odoo>
    <record id="view_users_form_inherit_estate" model="ir.ui.view">
        <field name="name">res.users.form.inherit.estate</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='page_security']" position="after">
                <page name='properties' string="Property">
                    <field name="property_ids"/>
                </page>
            </xpath>

        </field>
    </record>
</odoo>